{
    "configuration": {
        "appenders": {
            "Console": {
                "name": "STDOUT",
                "PatternLayout": {
                    "pattern": "%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n"
                }
            }
        },
        "loggers": {
            "root": {
                "level": "debug",
                "appender-ref": {
                    "ref": "STDOUT"
                }
            }
        }
    }
}